big main() {
	print func(1),
	print func(-1),
	print func(1*2*3*4*5),
	
	small svar1 svar2 << 5,
	print func(svar1),
	print func(svar2),
	print func(svar1 + 1),
	print func(svar1 * 2),
	print func(svar1 * svar2),
	print func(func(2+1)),
	print func(func(1)),
	
	print func(func(svar1)),
	print func(func(svar1 + 1)),
	print func(func(func((-1)))),
	
	normal nvar << func(1),
	print nvar,
	return 0.
}

big func(big svar1) {
	return -svar1.
}